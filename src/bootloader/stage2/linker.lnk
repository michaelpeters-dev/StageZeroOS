FORMAT RAW BIN                    ; output a flat raw binary (no headers, no relocation info)

OPTION QUIET,                     
       NODEFAULTLIBS,             
       START=entry,               
       VERBOSE,                   
       OFFSET=0,                  
       STACK=0x200                

ORDER                             
    CLNAME CODE                   
        SEGMENT _ENTRY            
        SEGMENT _TEXT             

    CLNAME DATA                 